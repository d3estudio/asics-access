<body onload="window.print()">
<table width="100%" bgcolor="#5D5D5D" cellpadding="0" cellspacing="0" border="0" class="backgroundTable"
       st-sortable="full-text">
  <tbody>
  <tr>
    <td>
      <table width="600" cellpadding="0" cellspacing="0" border="0" align="center" class="devicewidth">
        <tbody>
        <tr>
          <td width="100%">
            <table width="600" cellpadding="0" cellspacing="0" border="0" align="center"
                   class="devicewidth">
              <tbody>
              <tr>
                <td height="5" style="font-size:1px; line-height:1px; mso-line-height-rule: exactly;">&nbsp;</td>
              </tr>
              <tr>
                <td>
                  <table width="580" align="center" cellpadding="0" cellspacing="0" border="0"
                         class="devicewidthinner">
                    <tbody>
                    <tr>
                      <td align="center">
                        <img src="http://asicshub.com.br/images/email/asics-qrcode_top.jpg"
                             alt="WANT IT MORE" width="580"
                             style="display:block; width:100%; margin:0 auto"/>
                      </td>
                    </tr>
                    </tbody>
                  </table>
                </td>
              </tr>
              </tbody>
            </table>
          </td>
        </tr>
